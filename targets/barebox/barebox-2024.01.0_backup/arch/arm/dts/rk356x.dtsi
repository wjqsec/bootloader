/ {
	chosen {
		barebox,bootsource-mmc0 = &sdhci;
		barebox,bootsource-mmc1 = &sdmmc0;
		barebox,bootsource-mmc2 = &sdmmc1;
	};

	dmc: memory-controller {
		compatible = "rockchip,rk3568-dmc";
		rockchip,pmu = <&pmugrf>;
	};

	otp: nvmem@fe38c000 {
		compatible = "rockchip,rk3568-otp";
		reg = <0x0 0xfe38c000 0x0 0x4000>;
		#address-cells = <1>;
		#size-cells = <1>;

		cpu_id: id@a {
			reg = <0x0a 0x10>;
		};
	};
};
